<span class="kt">int</span> <span class="n">awz_num</span> <span class="o">=</span> <span class="n">var_num</span> <span class="o">+</span> <span class="n">ec_num</span><span class="p">;</span>
<span class="kt">int</span> <span class="n">total_num</span> <span class="o">=</span> <span class="n">var_num</span> <span class="o">+</span> <span class="n">ec_num</span> <span class="o">+</span> <span class="n">ic_num</span><span class="p">;</span>
