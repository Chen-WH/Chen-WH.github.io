<span class="kt">double</span> <span class="p">(</span><span class="o">*</span><span class="n">h</span><span class="p">[])(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[])</span> <span class="o">=</span> <span class="p">{</span> <span class="n">h0</span> <span class="p">};</span>
<span class="kt">double</span> <span class="p">(</span><span class="o">*</span><span class="n">g</span><span class="p">[])(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[])</span> <span class="o">=</span> <span class="p">{</span> <span class="n">g0</span><span class="p">,</span> <span class="n">g1</span> <span class="p">};</span>
<span class="kt">double</span> <span class="p">(</span><span class="o">*</span><span class="n">df</span><span class="p">[])(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[])</span> <span class="o">=</span> <span class="p">{</span> <span class="n">df0</span><span class="p">,</span> <span class="n">df1</span> <span class="p">};</span>
<span class="kt">double</span> <span class="p">(</span><span class="o">*</span><span class="n">dh</span><span class="p">[])(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[])</span> <span class="o">=</span> <span class="p">{</span> <span class="n">dh0</span><span class="p">,</span> <span class="n">dh1</span> <span class="p">};</span>
<span class="kt">double</span> <span class="p">(</span><span class="o">*</span><span class="n">dg</span><span class="p">[])(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[])</span> <span class="o">=</span> <span class="p">{</span> <span class="n">dg00</span><span class="p">,</span> <span class="n">dg01</span><span class="p">,</span> <span class="n">dg10</span><span class="p">,</span> <span class="n">dg11</span> <span class="p">};</span>
<span class="kt">double</span> <span class="p">(</span><span class="o">*</span><span class="n">Qdf</span><span class="p">[])(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[])</span> <span class="o">=</span> <span class="p">{</span> <span class="n">Qdf00</span><span class="p">,</span> <span class="n">Qdf01</span><span class="p">,</span> <span class="n">Qdf10</span><span class="p">,</span> <span class="n">Qdf11</span> <span class="p">};</span>


<span class="c1">//System define</span>
<span class="kt">int</span> <span class="n">c_num</span> <span class="o">=</span> <span class="n">ec_num</span> <span class="o">+</span> <span class="n">ic_num</span><span class="p">;</span>
<span class="kt">int</span> <span class="n">awz_num</span> <span class="o">=</span> <span class="n">var_num</span> <span class="o">+</span> <span class="n">ec_num</span><span class="p">;</span>
<span class="kt">int</span> <span class="n">total_num</span> <span class="o">=</span> <span class="n">var_num</span> <span class="o">+</span> <span class="n">ec_num</span> <span class="o">+</span> <span class="n">ic_num</span><span class="p">;</span>
<span class="kt">int</span> <span class="n">active_num</span><span class="p">;</span>

<span class="c1">//gradientå‡½æ•°</span>
<span class="n">MatrixXd</span> <span class="nf">Gradient</span><span class="p">(</span><span class="kt">double</span> <span class="n">x</span><span class="p">[],</span> <span class="kt">int</span> <span class="n">activeset</span><span class="p">[])</span> <span class="p">{</span>
	<span class="n">MatrixXd</span> <span class="n">tmp</span><span class="p">(</span><span class="n">awz_num</span> <span class="o">+</span> <span class="n">active_num</span><span class="p">,</span> <span class="mi">1</span><span class="p">);</span>
	<span class="kt">int</span> <span class="n">count</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
	<span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">var_num</span><span class="p">;</span> <span class="o">++</span><span class="n">i</span><span class="p">)</span> <span class="p">{</span>
