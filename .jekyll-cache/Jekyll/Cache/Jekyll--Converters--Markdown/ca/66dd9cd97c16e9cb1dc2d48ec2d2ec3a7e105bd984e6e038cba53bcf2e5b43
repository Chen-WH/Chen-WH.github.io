  <span class="k">while</span> <span class="p">(</span><span class="n">ros</span><span class="o">::</span><span class="n">ok</span><span class="p">())</span> <span class="c1">// roscppå°†å®‰è£…ä¸€ä¸ªSIGINTå¤„ç†ç¨‹åºï¼Œå®ƒèƒ½å¤Ÿå¤„ç†Ctrl+Cæ“ä½œï¼Œè®©ros::ok()è¿”å›falseã€‚</span>
  <span class="p">{</span>
    <span class="n">std_msgs</span><span class="o">::</span><span class="n">String</span> <span class="n">msg</span><span class="p">;</span>

    <span class="n">std</span><span class="o">::</span><span class="n">stringstream</span> <span class="n">ss</span><span class="p">;</span>
